{ "name" : "webinos_platform"
, "version" : "0.6.0"
, "description" : "Web runtime platform based on webinos specification"
, "keywords" :
  [ "webinos"
  , "api"
  ]
, "homepage" : "http://www.webinos.org"
, "bugs" : "http://jira.webinos.org"
, "repository" :
  { "type" : "git"
  , "url" : "http://github.com/webinos/Webinos-Platform.git"
  }
, "dependencies" :
  { "webinos-jsonrpc2" : "*"
  , "schema" : "0.x.x"
  , "sax" : "0.x.x"
  , "seq" : "0.x.x"
  , "xml2js" : "0.x.x"
  , "xml2json" : ">= 0.3.x"
  , "data2xml" : "0.8.x"
  , "crypt" : "0.0.x"
  , "ejs" : "0.x.x"
  , "openid" : "0.x.x"
  , "websocket" : "1.0.2"
  , "ansi" : "0.0.x"
  , "mdns" : "1.x.x"
  , "now" : "0.8.1"
  , "node-uuid" : "1.3.x"
  , "find-dependencies" : "~0.0.2"
  , "inherits" : "1.0.0"
  , "mkdirp" : "0.3.x"
  , "connect" : "2.6.x"
  , "grunt-contrib-clean" : "~0.3.x"
  , "express": "3.x"
  }
, "optionalDependencies" :
  { "sqlite3" : "2.1.x"
  , "qrcode" : "0.x.x"
  }
, "engines" :
  { "node" : ">= 0.6.0"
  , "npm" : ">= 1.1.17"
  }
, "preferGlobal": true
, "bin":
  { "webinos_pzh": "webinos_pzh.js"
  , "webinos_pzp": "webinos_pzp.js"
  }
, "scripts":
  { "install": "node-gyp configure build && cd webinos && grunt && cd .."
  }
}
